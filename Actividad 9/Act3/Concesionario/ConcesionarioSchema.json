{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "Concesionario":{
        "coche":{
            "codCoche":{
                "type":"string",
                "minLength": 1
            }, "marca":{
                "type":"string",
                "minLength": 1
            }, "modelo":{
                "type":"string",
                "minLength": 1
            }, "matricula":{
                "type":"string",
                "minLength": 1
            }, "potencia":{
                "caballos":{
                    "type":"integer"
                }
            },"required":["codCoche", "marca", "modelo", "caballos"] 
            ,"plazas":{
                "type":"integer"
            }, "numPuertas":{
                "type":"integer"
            }
            }, "required":["plazas", "numPuertas"]
    }
}