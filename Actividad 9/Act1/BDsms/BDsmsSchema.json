{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "BDsms":{
        "sms":{
            "telefono":{
                "type":"string",
                "minLength": 1
            }, "fecha":{
                "type":"string", "format":"date"
            }, "hora":{
